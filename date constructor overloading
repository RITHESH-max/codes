
#include<iostream>
using namespace std;

class date
{
    private:
        int dd, mm, yy;

    public:
        date()
        {
            dd=01;
            mm=02;
            yy=2002;
        }
        date(int date, int month, int year){
            this->dd = date;
            this->mm = month;
            this->yy = year;
        }

        void display()
        {
            cout<<"\nThe Entered Date is :: ";
            cout<<dd<<"-"<<mm<<"-"<<yy<<"\n";
        }
};
int main ()
{
date obj,obj2(25,1,2003);
obj.display ();
obj2.display();

return 0;
}
